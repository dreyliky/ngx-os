    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.7.0",
    "ng-packagr": "^12.2.1",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "typescript": "4.2.4"
